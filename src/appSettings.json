{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      }
      ,
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "RollingFile",
                "Args": {
                  "pathFormat": "log.{Date}.log",
                  "retainedFileCountLimit": 5
                }
              }
            ]
          }
        }
      }
    ]
  }
}