{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      "Console",
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "RollingFile",
                "Args": {
                  "pathFormat": "log.{Date}.log",
                  "retainedFileCountLimit": 5
                }
              }
            ]
          }
        }
      }
    ]
  }
}